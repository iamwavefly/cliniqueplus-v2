<div class="main__panel">
    <div class="user_welcome-mobile">
        <h3>Welcome Back!</h3>
        <h2>Samson Monday</h2>
    </div>
    <div class="search-mobile">
        <input type="text" placeholder="Search for patient">
        <button type="submit" class="mobile__search-btn">
            <img src="./assets/icons/search_mobile.svg">
        </button>
    </div>
    <div class="card__title-mobile">
        <h3>HolyLand Overview</h3>
    </div>
    <div class="cards">
        <div class="card-1">
            <div class="card__icon">
                <img src="./assets/icons/16.svg" alt="">
            </div>
            <div class="card__content">
                <h3>2,260</h3>
                <p>Total Patients</p>
            </div>
            <img class="card__more__action" src="./assets/icons/27.svg" alt="" srcset="">
        </div>
        <div class="card-2">
            <div class="card__icon">
                <img src="./assets/icons/19.svg" alt="">
            </div>
            <div class="card__content">
                <h3>460</h3>
                <p>Available Staff</p>
            </div>
            <img class="card__more__action" src="./assets/icons/27.svg" alt="" srcset="">
        </div>
        <div class="card-3">
            <div class="card__icon">
                <img src="./assets/icons/18.svg" alt="">
            </div>
            <div class="card__content">
                <h3>4,860</h3>
                <p>Avg. Treat. Cost</p>
            </div>
            <img class="card__more__action" src="./assets/icons/27.svg" alt="" srcset="">
        </div>
        <div class="card-4">
            <div class="card__icon">
                <img src="./assets/icons/17.svg" alt="">
            </div>
            <div class="card__content">
                <h3>60</h3>
                <p>Available Cars</p>
            </div>
            <img class="card__more__action" src="./assets/icons/27.svg" alt="" srcset="">
        </div>
    </div>
    <div class="charts">
        <div class="chart-1">
            <div class="chart-l">
                <div class="chart__header">
                    <h3>Outpatient vs Impatient Trend</h3>
                </div>
                <div class="chart__container">
                    <canvas id="myChart" width="400" height="250" class="chart"></canvas>
                </div>
            </div>
        </div>
        <div class="chart-2">
            <div class="chart__header">
                <h3>Inpatient vs Outpatient by %</h3>
            </div>
            <div class="clearfix cirle__w__percentage">

                <div class="c100 p70 normal primary">
                    <span>70%</span>
                    <div class="slice">
                        <div class="bar"></div>
                        <div class="fill"></div>
                    </div>
                </div>
            </div>
            <div class="chart__footer">
                <h3>Inpatient</h3>
                <h3>Outpatient</h3>
            </div>
        </div>
        <div class="chart-3">
            <div class="chart__header">
                <h3>Income vs Expenses Trend</h3>
            </div>
            <div class="chart__container">
                <canvas id="chart__finance" width="400" height="250" class="chart"></canvas>
            </div>
        </div>
        <div class="chart-4">
            <div class="chart__header">
                <h3>Patient by Division <img src="./assets/icons/28.svg" alt="" srcset=""></h3>
                
            </div>
            <div class="content">
                <div class="div__header">
                        <h3><img src="./assets/icons/20.svg" alt="" srcset=""> DIVISION</h3>
                        <h3>PATIENT</h3>
                </div>
                <div class="div__content">
                    <p><img src="./assets/icons/21.svg" alt="" srcset=""> Cardiolody</p>
                    <p>45</p>
                    <p><img src="./assets/icons/22.svg" alt="" srcset=""> Neurology</p>
                    <p>22</p>
                    <p><img src="./assets/icons/23.svg" alt="" srcset=""> Surgery</p>
                    <p>22</p>
                </div>
            </div>
        </div>
        <div class="chart-5">
            <div class="header">
                <h3>340,000</h3>
                <p>Patients this month</p>
            </div>
            <div class="chart__container">
                <canvas id="chart-5" width="400" height="250" class="chart"></canvas>
            </div>
        </div>
    </div>
</div>